             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Halo_Forge_Bot.Windows.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="Transparent">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        
        <StackPanel>
            <Button Name="CollectItemData" Click="CollectItemData_OnClick" >Collect Item Data</Button>
            <Button Name="CollectSavedCategoryInfo" Click="CollectSavedCategoryInfo_OnClick" >Collect Saved Category Info</Button>
        </StackPanel>
        <StackPanel Grid.Column="1">
            
        </StackPanel>
        
    </Grid>
    
    
   
</UserControl>
